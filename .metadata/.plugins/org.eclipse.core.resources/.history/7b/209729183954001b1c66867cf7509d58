// Exibir lista de numeros primos até o limite definido pelo usuario

package estudoJava;
import java.util.Scanner;

public class primoLista {

	public static void main(String[] args) {
		var sc = new Scanner(System.in);
		
		int limite;
		int contador=0;
		for(int i=0; i<limite; i++) {
			
			if(limite %i==0) {
				contador++;
			}
			
		}
		

sc.close();
	}

}
