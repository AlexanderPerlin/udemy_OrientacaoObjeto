// Exibir lista de numeros primos até o test definido pelo usuario

package estudoJava;
import java.util.Scanner;

public class primoLista {

	public static void main(String[] args) {
		var sc = new Scanner(System.in);
		
		int test,limite;
		int contador = 0;
		
		System.out.println("digite o limite");
		limite = sc.nextInt();
		sc.close();
		
	for(int y = 1; y<=limite; y++) {
		test=y;
		
		for(int i=1; i<=test; i++) {
			if(test %i==0) {
				contador++;				
			}
			if(contador == 2) {
				System.out.println(test);
						
			}
		}
		
	}
		
	}

}
