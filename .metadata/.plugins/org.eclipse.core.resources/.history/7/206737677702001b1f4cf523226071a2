public class Primo{
	
	public static void main(String[] args) {
	int i, result;	
	
	for (i=2; i < 100; i++) {
		
	//	if (i %2 !=0) {
		
		result = i %2;
			
		System.out.println(result);	
		
	}
				
	}
		
}
